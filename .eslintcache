ultado = {};\n      await fetch(url)\n        .then((response) => response.json())\n        .then((data) => (resultado = data));\n      validarRenderPelicula(resultado);\n    } catch (error) {\n      console.log(error);\n    }\n  }\n\n  //funcion para el caso en que la busqueda no devuelva ningun resultado.\n\n  function validarRenderPelicula(resultado) {\n    if (resultado.total_results === 0) {\n      alert(\"no encontre nadia\");\n    } else {\n      setResultadoBusqueda(resultado);\n    }\n  }\n\n\n  \n\n  return (\n    <Fragment>\n    <Paper elevation={3} className={classes.paper}>\n      <form noValidate autoComplete=\"off\">\n        <TextField\n          className={classes.textfield}\n          id=\"standard-basic\"\n          name = \"nombre\"\n          label=\"Nombre de la pelicula\"\n          variant=\"outlined\"\n          value = {nombre}\n          onChange={handleChange}\n        />\n        <TextField\n          className={classes.textfield}\n          id=\"standard-basic\"\n          name= \"anio\"\n          label=\"AÃ±o de la pelicula\"\n          variant=\"outlined\"\n          type =\"number\"\n          value = {anio}\n          onChange={handleChange}\n        />\n        <Button variant=\"contained\" color=\"primary\" className={classes.button} onClick={realizarBusqueda}>\n          Buscar\n        </Button>\n      </form>\n    </Paper>\n    <ListaDePeliculas resultadoBusqueda= {resultadoBusqueda}/>\n    \n    </Fragment> \n  );\n};\n\nexport default withStyles(styles)(BusquedaPorNombre);\n","/home/leonelp/repos/infomovies/src/components/resources/buttonsData.js",[],"/home/leonelp/repos/infomovies/src/components/Home.jsx",[],"/home/leonelp/repos/infomovies/src/components/Pelicula.jsx",["61","62"],"/home/leonelp/repos/infomovies/src/components/BusquedaAleatoria.jsx",[],"/home/leonelp/repos/infomovies/src/components/ListaDePeliculas.jsx",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"messag